<odoo>
    <data>
        <!--Vista heredada para compaÃ±ias-->
        <record id="tracker_company_inherit_form" model="ir.ui.view">
            <field name="name">tracker_company_inherit_form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//notebook" position="inside" groups="base.group_system">
                        <page name="gps_info" string="Configuracion GPS">
                            <group>
                                <field name="gps_url"/>
                                <field name="gps_hash"/>
                            </group>
                        </page>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>